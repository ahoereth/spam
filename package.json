{
  "name": "cogsci-spam",
  "version": "1.7.0",
  "author": "Alexander Hoereth <ahoereth@uos.de>",
  "homepage": "https://cogsci.uos.de/~SPAM",
  "repository": {
    "type": "git",
    "url": "http://github.com/ahoereth/spam.git"
  },
  "bugs": "http://github.com/ahoereth/spam/issues",
  "scripts": {
    "clean": "rm -r dev app",
    "build": "rollup -c --environment NODE_ENV:production",
    "servdev": "http-server dev --push-state",
    "watch": "rollup -c --watch",
    "reload": "livereload 'app/' -d",
    "start": "npm-run-all -p reload watch",
    "start:remote": "npm-run-all -p reload 'watch -- --environment API:remote'",
    "start:uglify": "npm-run-all -p reload 'watch -- --environment UGLIFY:1'",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "dependencies": {
    "Base64": "^1.0.0",
    "angular": "1.5.x",
    "angular-route": "1.5.x",
    "angular-sanitize": "1.5.x",
    "angular-tooltips": "^1.1.7",
    "bootstrap": "^3.3.7",
    "lodash": "^4.15.0",
    "lodash-es": "^4.16.2",
    "open-sans-fontface": "^1.4.0",
    "restangular": "^1.5.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "fs-extra": "^2.0.0",
    "less-plugin-autoprefix": "1.5.x",
    "less-plugin-clean-css": "^1.5.1",
    "less-plugin-npm-import": "^2.1.0",
    "livereload": "^0.6.0",
    "npm-run-all": "^4.0.0",
    "rollup": "0.41.1",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-less": "^0.1.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2",
    "spa-http-server": "^0.9.0"
  }
}
